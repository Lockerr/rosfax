!!!
%html
  %head
    %title Tradein
    = stylesheet_link_tag    "application"
    = javascript_include_tag "application"
    = csrf_meta_tags
  %body
    - if @report and !@report.new_record?
      .container-fluid#report{'report' => @report.id, :source => @report.attributes.to_json}
        = yield
    - else
      .container-fluid
        = yield

