!!!
%html
  %head
    %title Tradein
    = stylesheet_link_tag    "application"
    = javascript_include_tag "application"
    = csrf_meta_tags
  %body
    - unless @report.new_record?
      .container#report{'report' => @report.id, :source => @report.attributes.to_json}
        = yield
    - else
      .container
        = yield

